      		android:exported="false" />
         
        <activity
            android:name=".PracticalTest01Var03MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Practicaltest01Var03PlayActivity"
            android:label="@string/title_activity_practicaltest01_var03_play" >
            
            <intent-filter>
                <action android:name="ro.pub.cs.systems.eim.practicaltest.android.intent.action.PracticalTestSecondaryActivity" />
                
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>
    </application>

</manifest>
